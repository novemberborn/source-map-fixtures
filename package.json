{
  "name": "source-map-fixtures",
  "version": "2.1.0",
  "description": "Simple test fixtures backed by inline source maps and source map files",
  "keywords": [
    "source-map",
    "source-maps",
    "test",
    "testing",
    "fixture",
    "fixtures"
  ],
  "author": "Mark Wubben (https://novemberborn.net/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/novemberborn/source-map-fixtures"
  },
  "homepage": "https://github.com/novemberborn/source-map-fixtures",
  "bugs": "https://github.com/novemberborn/source-map-fixtures/issues",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "fixtures",
    "src",
    "index.js"
  ],
  "scripts": {
    "coverage": "nyc npm test",
    "generate": "node bin/generate.js",
    "test": "ava",
    "posttest": "standard"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-core": "6.4.5",
    "babel-plugin-transform-es2015-arrow-functions": "6.4.0",
    "babel-plugin-transform-es2015-block-scoping": "6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.4.5",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "glob": "^6.0.4",
    "md5-hex": "^1.2.1",
    "nyc": "^5.5.0",
    "proxyquire": "^1.7.3",
    "rimraf": "^2.5.1",
    "rollup": "0.25.2",
    "sinon": "^2.0.0-pre",
    "standard": "^5.4.1"
  },
  "standard": {
    "ignore": [
      "fixtures/"
    ]
  },
  "nyc": {
    "exclude": [
      "src",
      "fixtures"
    ]
  }
}
