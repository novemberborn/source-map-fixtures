{
  "name": "source-map-fixtures",
  "version": "2.1.0",
  "description": "Simple test fixtures backed by inline source maps and source map files",
  "keywords": [
    "source-map",
    "source-maps",
    "test",
    "testing",
    "fixture",
    "fixtures"
  ],
  "author": "Mark Wubben (https://novemberborn.net/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/novemberborn/source-map-fixtures"
  },
  "homepage": "https://github.com/novemberborn/source-map-fixtures",
  "bugs": "https://github.com/novemberborn/source-map-fixtures/issues",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "fixtures",
    "src",
    "index.js"
  ],
  "scripts": {
    "coverage": "nyc npm test",
    "generate": "node bin/generate.js",
    "test": "ava",
    "posttest": "standard"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-core": "6.7.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.5.2",
    "babel-plugin-transform-es2015-block-scoping": "6.7.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.7.0",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "glob": "^7.0.3",
    "md5-hex": "^1.2.1",
    "nyc": "^6.0.0",
    "proxyquire": "^1.7.4",
    "rimraf": "^2.5.2",
    "rollup": "0.25.6",
    "sinon": "^1.17.3",
    "standard": "^6.0.8"
  },
  "standard": {
    "ignore": [
      "fixtures/",
      "src/"
    ]
  },
  "nyc": {
    "exclude": [
      "fixtures",
      "src",
      "test"
    ]
  },
  "ava": {
    "files": "test/*.js",
    "source": [
      "*.js",
      "package.json",
      "fixtures/*.js",
      "src/*.js"
    ]
  }
}
